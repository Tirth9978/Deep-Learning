        "import torch\n",
        "import torch.nn as nn\n",
        "\n",
        "x = torch.randn(1, 3, 32, 32)\n",
        "\n",
        "block = nn.Sequential(\n",
        "    nn.Conv2d(3, 8, kernel_size=3, padding=1),\n",
        "    nn.ReLU(),\n",
        "    nn.MaxPool2d(2)\n",
        ")\n",
        "\n",
        "y = block(x)\n",
        "\n",
        "print(y.shape)\n"
      ],
