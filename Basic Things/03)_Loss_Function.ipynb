        "import torch\n",
        "import torch.nn as nn"
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "## Create a Linear Model"
      ],
      "metadata": {
        "id": "6ve-vMluDm0g"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "model = nn.Linear(2,1)\n",
        "\n",
        "x = torch.tensor([2.0 , 3])\n",
        "y_true = torch.tensor([5.0])\n",
        "\n",
        "y_pred = model(x)\n",
        "\n",
        "loss_fn = nn.MSELoss()\n",
        "loss = loss_fn(y_pred, y_true)\n",
        "loss"
      ],
